import java.util.Arrays;
import java.util.Scanner;
public class vetores {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int[] idade = new int[5];




        for (int i = 0 ; i <= 4; i++){
            System.out.println("Insira uma idade: ");
            idade[i] = scan.nextInt();
        }




    }
}
