import java.util.Arrays;
public class matriz {
    public static void main(String[] args) {
        int[][] tabuleiro = new int[3][3];
        tabuleiro [0][1] = 1;

        System.out.println(Arrays.deepToString(tabuleiro));

        for(int i = 0; i< 3; i++){
            System.out.println(Arrays.toString(tabuleiro[i]));
        }


    }
}
